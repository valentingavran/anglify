{
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main",
    "start": "node ./scripts/css-variable-extractor.js && yarn run compodoc-json && nx serve",
    "build": "node ./scripts/css-variable-extractor.js && yarn run compodoc-json && nx build",
    "test": "nx test --project=anglify",
    "test:coverage": "nx test --coverage --project=anglify",
    "lint": "prettier --check . && stylelint \"**/*.scss\" && nx run-many --all --target=lint",
    "format": "prettier --write . && stylelint \"**/*.scss\" --fix && nx run-many --all --target=lint --fix",
    "build:anglify": "rimraf dist/ && nx run anglify:build:production --skip-nx-cache && cpy . '../' --cwd=dist/libs/anglify/src && rimraf dist/libs/anglify/src",
    "release": "standard-version",
    "prepare": "is-ci || husky install",
    "compodoc-json": "compodoc -p ./libs/anglify/tsconfig.json -e json -d ./apps/docs/src/assets",
    "update": "yarn upgrade-interactive"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.2.0",
    "@angular/cdk": "^14.2.0",
