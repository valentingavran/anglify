{
  "name": "anglify",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": " node ./scripts/css-variable-extractor.js && ng build docs",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "lint": "prettier --check . && eslint projects --ext .js,.ts",
    "format": "prettier --write . && eslint projects --ext .js,.ts --fix",
    "docs:json": "compodoc -p ./tsconfig.json -e json -d .",
    "build:anglify": "ng build anglify && rsync -av dist/anglify/src/ dist/anglify/ && rm -rf dist/anglify/src"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^13.3.1",
    "@angular/common": "^13.3.1",
    "@angular/compiler": "^13.3.1",
    "@angular/core": "^13.3.1",
    "@angular/elements": "^13.3.1",
    "@angular/forms": "^13.3.1",
    "@angular/platform-browser": "^13.3.1",
    "@angular/platform-browser-dynamic": "^13.3.1",
    "@angular/router": "^13.3.1",
    "@ngneat/until-destroy": "^9.0.1",
    "highlight.js": "^11.5.0",
    "ngx-highlightjs": "^6.1.2",
    "ngx-markdown": "^13.1.0",
    "rxjs": "^7.5.5",
    "tslib": "^2.3.1",
    "zone.js": "^0.11.5"
  },
  "devDependencies": {
    "@anexia/eslint-config-marine": "^4.1.1",
    "@angular-devkit/build-angular": "^13.3.1",
    "@angular-eslint/builder": "^13.2.0",
    "@angular-eslint/eslint-plugin": "^13.2.0",
    "@angular-eslint/eslint-plugin-template": "^13.2.0",
    "@angular-eslint/schematics": "^13.2.0",
    "@angular-eslint/template-parser": "^13.2.0",
    "@angular-eslint/utils": "^13.2.0",
    "@angular/cli": "^13.3.1",
    "@angular/compiler-cli": "^13.3.1",
    "@babel/core": "^7.17.8",
    "@compodoc/compodoc": "^1.1.19",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@mdi/js": "^6.6.96",
    "@types/jasmine": "^4.0.2",
    "@types/node": "^17.0.23",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "@webcomponents/custom-elements": "^1.5.0",
    "babel-loader": "^8.2.4",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "glob": "^7.2.0",
    "jasmine-core": "^4.0.1",
    "karma": "^6.3.17",
    "karma-chrome-launcher": "^3.1.1",
    "karma-coverage": "^2.2.0",
    "karma-jasmine": "^4.0.2",
    "karma-jasmine-html-reporter": "^1.7.0",
    "ng-packagr": "^13.3.0",
    "prettier": "^2.6.2",
    "typescript": "^4.6.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/valentingavran/anglify.git"
  }
}
